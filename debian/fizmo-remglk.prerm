#! /bin/sh
# prerm script for fizmo-remglk

set -e

case "$1" in
    remove|upgrade|deconfigure)
        update-alternatives --remove zcode-interpreter /usr/games/fizmo-remglk-launcher
        ;;
    failed-upgrade)
        update-alternatives --remove zcode-interpreter /usr/games/fizmo-remglk-launcher
        ;;
    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 0
    ;;
esac

#DEBHELPER#

exit 0
